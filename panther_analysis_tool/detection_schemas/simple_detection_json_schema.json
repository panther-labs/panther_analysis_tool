{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$ref": "#/$defs/SimpleDetectionSchema",
  "$defs": {
    "AssociatedLogType": {
      "properties": {
        "LogType": {
          "type": "string"
        },
        "Selectors": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": ["LogType", "Selectors"]
    },
    "LogTypeMap": {
      "properties": {
        "PrimaryKey": {
          "type": "string"
        },
        "AssociatedLogTypes": {
          "items": {
            "$ref": "#/$defs/AssociatedLogType"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": ["PrimaryKey", "AssociatedLogTypes"]
    },
    "Mapping": {
      "properties": {
        "Name": {
          "type": "string"
        },
        "Method": {
          "type": "string"
        },
        "Path": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": ["Name"]
    },
    "MatchExpressions": {
      "items": {
        "oneOf": [
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "Expressions": {
                "$ref": "#/$defs/MatchExpressions"
              },
              "Key": {
                "type": "string"
              },
              "Condition": {
                "type": "string",
                "enum": ["AllElements", "AnyElement", "NoElement", "OnlyOneElement"]
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["Expressions", "Key", "Condition"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "Expressions": {
                "$ref": "#/$defs/MatchExpressions"
              },
              "DeepKey": {
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              "Condition": {
                "type": "string",
                "enum": ["AllElements", "AnyElement", "NoElement", "OnlyOneElement"]
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["Expressions", "DeepKey", "Condition"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "Expressions": {
                "$ref": "#/$defs/MatchExpressions"
              },
              "PathSpecifier": {
                "type": "string"
              },
              "Condition": {
                "type": "string",
                "enum": ["AllElements", "AnyElement", "NoElement", "OnlyOneElement"]
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["Expressions", "PathSpecifier", "Condition"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "All": {
                "$ref": "#/$defs/MatchExpressions"
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["All"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "Any": {
                "$ref": "#/$defs/MatchExpressions"
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["Any"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "OnlyOne": {
                "$ref": "#/$defs/MatchExpressions"
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["OnlyOne"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "Condition": {
                "type": "string",
                "enum": ["AlwaysTrue", "AlwaysFalse"]
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["Condition"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "Key": {
                "type": "string"
              },
              "Condition": {
                "type": "string"
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["Key", "Condition"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "DeepKey": {
                "items": {
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "integer"
                    }
                  ]
                },
                "type": "array"
              },
              "Condition": {
                "type": "string"
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["DeepKey", "Condition"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "PathSpecifier": {
                "type": "string"
              },
              "Condition": {
                "type": "string"
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["PathSpecifier", "Condition"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "Key": {
                "type": "string"
              },
              "Condition": {
                "type": "string"
              },
              "Value": {
                "anyOf": [
                  {
                    "type": "number"
                  },
                  {
                    "type": "string"
                  },
                  {
                    "type": "integer"
                  },
                  {
                    "type": "boolean"
                  }
                ],
                "additionalProperties": false
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["Key", "Condition", "Value"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "Key": {
                "type": "string"
              },
              "Condition": {
                "type": "string"
              },
              "Values": {
                "anyOf": [
                  {
                    "items": {
                      "type": "number"
                    },
                    "type": "array"
                  },
                  {
                    "items": {
                      "type": "string"
                    },
                    "type": "array"
                  },
                  {
                    "items": {
                      "type": "integer"
                    },
                    "type": "array"
                  },
                  {
                    "items": {
                      "type": "boolean"
                    },
                    "type": "array"
                  }
                ]
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["Key", "Condition", "Values"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "DeepKey": {
                "items": {
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "integer"
                    }
                  ]
                },
                "type": "array"
              },
              "Condition": {
                "type": "string"
              },
              "Value": {
                "anyOf": [
                  {
                    "type": "number"
                  },
                  {
                    "type": "string"
                  },
                  {
                    "type": "integer"
                  },
                  {
                    "type": "boolean"
                  }
                ],
                "additionalProperties": false
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["DeepKey", "Condition", "Value"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "DeepKey": {
                "items": {
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "integer"
                    }
                  ]
                },
                "type": "array"
              },
              "Condition": {
                "type": "string"
              },
              "Values": {
                "anyOf": [
                  {
                    "items": {
                      "type": "number"
                    },
                    "type": "array"
                  },
                  {
                    "items": {
                      "type": "string"
                    },
                    "type": "array"
                  },
                  {
                    "items": {
                      "type": "integer"
                    },
                    "type": "array"
                  },
                  {
                    "items": {
                      "type": "boolean"
                    },
                    "type": "array"
                  }
                ]
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["DeepKey", "Condition", "Values"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "PathSpecifier": {
                "type": "string"
              },
              "Condition": {
                "type": "string"
              },
              "Value": {
                "anyOf": [
                  {
                    "type": "number"
                  },
                  {
                    "type": "string"
                  },
                  {
                    "type": "integer"
                  },
                  {
                    "type": "boolean"
                  }
                ],
                "additionalProperties": false
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["PathSpecifier", "Condition", "Value"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "PathSpecifier": {
                "type": "string"
              },
              "Condition": {
                "type": "string"
              },
              "Values": {
                "anyOf": [
                  {
                    "items": {
                      "type": "number"
                    },
                    "type": "array"
                  },
                  {
                    "items": {
                      "type": "string"
                    },
                    "type": "array"
                  },
                  {
                    "items": {
                      "type": "integer"
                    },
                    "type": "array"
                  },
                  {
                    "items": {
                      "type": "boolean"
                    },
                    "type": "array"
                  }
                ]
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["PathSpecifier", "Condition", "Values"]
          },
          {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "properties": {
              "Condition": {
                "type": "string"
              },
              "Values": {
                "items": {
                  "oneOf": [
                    {
                      "$schema": "https://json-schema.org/draft/2020-12/schema",
                      "properties": {
                        "Key": {
                          "type": "string"
                        }
                      },
                      "additionalProperties": false,
                      "type": "object",
                      "required": ["Key"]
                    },
                    {
                      "$schema": "https://json-schema.org/draft/2020-12/schema",
                      "properties": {
                        "DeepKey": {
                          "items": {
                            "oneOf": [
                              {
                                "type": "string"
                              },
                              {
                                "type": "integer"
                              }
                            ]
                          },
                          "type": "array"
                        }
                      },
                      "additionalProperties": false,
                      "type": "object",
                      "required": ["DeepKey"]
                    }
                  ],
                  "type": "object"
                },
                "type": "array",
                "maxItems": 2,
                "minItems": 2
              }
            },
            "additionalProperties": false,
            "type": "object",
            "required": ["Condition", "Values"]
          }
        ],
        "type": "object"
      },
      "type": "array"
    },
    "QuerySchedule": {
      "properties": {
        "CronExpression": {
          "type": "string"
        },
        "RateMinutes": {
          "type": "integer"
        },
        "TimeoutMinutes": {
          "type": "integer"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": ["TimeoutMinutes"]
    },
    "Refresh": {
      "properties": {
        "RoleARN": {
          "type": "string"
        },
        "PeriodMinutes": {
          "type": "integer"
        },
        "AlarmPeriodMinutes": {
          "type": "integer"
        },
        "ObjectPath": {
          "type": "string"
        },
        "ObjectKMSKey": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": ["RoleARN", "PeriodMinutes", "AlarmPeriodMinutes", "ObjectPath", "ObjectKMSKey"]
    },
    "SimpleDetectionSchema": {
      "properties": {
        "AnalysisType": {
          "type": "string",
          "enum": [
            "datamodel",
            "global",
            "pack",
            "policy",
            "rule",
            "scheduled_rule",
            "scheduled_query",
            "lookup_table"
          ]
        },
        "Description": {
          "type": "string"
        },
        "DisplayName": {
          "type": "string"
        },
        "Enabled": {
          "type": "boolean"
        },
        "Filename": {
          "type": "string"
        },
        "LogTypes": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "OnlyUseBaseRiskScore": {
          "type": "boolean"
        },
        "Reference": {
          "type": "string"
        },
        "Reports": {
          "patternProperties": {
            ".*": {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          },
          "type": "object"
        },
        "Runbook": {
          "type": "string"
        },
        "Severity": {
          "type": "string"
        },
        "Tags": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "Tests": {
          "items": {
            "$ref": "#/$defs/Test"
          },
          "type": "array"
        },
        "PolicyID": {
          "type": "string"
        },
        "ResourceTypes": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "Suppressions": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "DataModelID": {
          "type": "string"
        },
        "Mappings": {
          "items": {
            "$ref": "#/$defs/Mapping"
          },
          "type": "array"
        },
        "DedupPeriodMinutes": {
          "type": "integer"
        },
        "OutputIds": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "RuleID": {
          "type": "string"
        },
        "SummaryAttributes": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "Threshold": {
          "type": "integer"
        },
        "ScheduledQueries": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "GlobalID": {
          "type": "string"
        },
        "AthenaQuery": {
          "type": "string"
        },
        "Query": {
          "type": "string"
        },
        "QueryName": {
          "type": "string"
        },
        "Schedule": {
          "$ref": "#/$defs/QuerySchedule"
        },
        "SnowflakeQuery": {
          "type": "string"
        },
        "LogTypeMap": {
          "$ref": "#/$defs/LogTypeMap"
        },
        "LookupName": {
          "type": "string"
        },
        "Refresh": {
          "$ref": "#/$defs/Refresh"
        },
        "Schema": {
          "type": "string"
        },
        "Detection": {
          "$ref": "#/$defs/MatchExpressions"
        },
        "InlineFilters": {
          "$ref": "#/$defs/MatchExpressions"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": ["AnalysisType"]
    },
    "Test": {
      "properties": {
        "ExpectedResult": {
          "type": "boolean"
        },
        "Log": true,
        "LogType": {
          "type": "string"
        },
        "Mocks": {
          "items": {
            "patternProperties": {
              ".*": {
                "type": "string"
              }
            },
            "type": "object"
          },
          "type": "array"
        },
        "Name": {
          "type": "string"
        },
        "Resource": true,
        "ResourceType": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": ["ExpectedResult", "Name"]
    }
  }
}
